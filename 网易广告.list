#!name=网易广告
#!desc=网易去广告
#!author=RuCu6[https://github.com/RuCu6], Keywos[https://github.com/Keywos]
#!icon=https://rx.nzvrn.com/x/netease-cloudmusic
#!homepage=https://hub.kelee.one
[Argument]
FX = switch, false, true, tag = 底部发现, desc = 是否隐藏底部发现标签
MY = switch, false, true, tag = 底部漫游, desc = 是否隐藏底部漫游标签
DT = switch, false, true, tag = 底部笔记, desc = 是否隐藏底部笔记标签
GZ = switch, false, true, tag = 底部关注, desc = 是否隐藏底部关注标签
PRGG = switch, false, true, tag = 首页问候, desc = 是否隐藏首页问候语
PRRK = switch, false, true, tag = 首页排行, desc = 是否隐藏首页排行榜
PRDRD = switch, false, true, tag = 首页每日, desc = 是否隐藏首页每日推荐
PRSCVPT = switch, false, true, tag = 首页推荐, desc = 是否隐藏首页推荐歌单
PRST = switch, false, true, tag = 首页常听, desc = 是否隐藏首页最近常听
PRRR = switch, false, true, tag = 首页雷达, desc = 是否隐藏首页雷达歌单
HMPR = switch, false, true, tag = 首页合伙, desc = 是否隐藏首页音乐合伙人
PRMST = switch, false, true, tag = 首页专属, desc = 是否隐藏首页专属歌单
PRCN = switch, false, true, tag = 首页你的, desc = 是否隐藏首页你的专属歌单

Music163_VIP_Shared = switch, false, true, tag = 共享会员, desc = 是否启用网易云音乐共享会员
Cookie = input, "", tag = Cookie, desc = 请填写Cookie值
MConfigInfo = input, "", tag = MConfigInfo ,desc = 请填写MConfigInfo值
UserAgent = input, "", tag = UserAgent, desc = 请填写UserAgent值

[MitM]
hostname = interface*.music.163.com, ipv4.music.163.com

[Rule]
DOMAIN,iadmat.nosdn.127.net,REJECT
DOMAIN,iadmatapk.nosdn.127.net,REJECT
DOMAIN,iadmusicmat.music.126.net,REJECT
DOMAIN,iadmusicmatvideo.music.126.net,REJECT
DOMAIN,ipv4.music.163.com,REJECT
DOMAIN,ipv6.music.163.com,REJECT

[Rewrite]
^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/e?api\/(?:batch|homepage\/block\/page|v\d\/(?:resource\/comment\/floor\/get|user\/detail\/\d+|discovery\/recommend\/songs|playlist\/detail)|link\/page\/rcmd\/(?:block\/resource\/multi\/refresh|resource\/show)|user\/follow\/users\/mixed\/get|link\/home\/framework\/tab|link\/position\/show\/resource|link\/page\/discovery\/resource\/show|mine\/(?:collect|rn)\/header\/info|vipnewcenter\/app\/resource\/newaccountpage|music-vip-membership\/(?:client|front)\/vip\/info|playlist\/privilege|search\/complex\/page) header-replace x-aeapi false
^https?:\/\/interface\d?\.music\.163\.com\/e?api\/vip\/cashier\/tspopup\/get reject-200

# 开屏广告
^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/e?api\/(ocpc\/)?ad\/ mock-response-body data-type=text

# 今日运势 商城 Beat专区 音乐收藏家 | type:ACTIVITY | 低至5.2折
^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/(?:delivery\/(batch-)?deliver|moment\/tab\/info\/|side-bar\/mini-program\/music-service\/account|yunbei\/account\/entrance\/) reject-dict

# 播放页 歌名下方 乐迷团｜关注｜播放页提示｜音乐应用红点｜播放提示
^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/(?:community\/friends\/fans-group\/artist\/group\/|mine\/applet\/redpoint|music\/songshare\/text\/recommend\/|resniche\/position\/play\/new\/|resniche\/tspopup\/show|resource\/comments?\/musiciansaid\/|user\/sub\/artist) reject-dict
^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/(?:ios\/version|mlivestream\/entrance\/playpage\/|link\/position\/show\/strategy|link\/scene\/show\/resource|v1\/content\/exposure\/comment\/banner\/) reject-dict

# 搜索页 搜索词 热搜卡片 猜你喜欢 我的应用下方提醒
^https?:\/\/(?:ipv4|interface\d?)\.music\.163.com\/w?e?api\/search\/default mock-response-body data-type=text
^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/w?eapi\/(?:activity\/bonus\/playpage\/time\/query|resource-exposure\/|search\/(?:chart\/|rcmd\/keyword\/|specialkeyword\/)) reject-dict

# 主页播客推荐
^https:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/my\/podcast\/tab\/recommend reject-dict

[Script]
#----------  会员解锁 ----------#
# 播放会员歌曲
http-request ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/(?:v1\/artist\/top\/song|v3\/discovery\/recommend\/songs) script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/CloudMusicVIP.js, requires-body = false, binary-body-mode = false, timeout = 20, tag = 网易播放, enable={Music163_VIP_Shared} ,argument=[{Cookie},{MConfigInfo},{UserAgent}]
http-request ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/v3\/song\/detail script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/CloudMusicVIP.js, requires-body = false, binary-body-mode = false, timeout = 20, tag = 播放会员, enable={Music163_VIP_Shared} ,argument=[{Cookie},{MConfigInfo},{UserAgent}]
http-request ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/song\/(?:chorus|enhance\/|play\/|type\/detail\/get) script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/CloudMusicVIP.js, requires-body = false, binary-body-mode = false, timeout = 20, tag = 会员歌曲, enable={Music163_VIP_Shared} ,argument=[{Cookie},{MConfigInfo},{UserAgent}]

# 播放器会员皮肤
http-request ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/playermode\/ script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/CloudMusicVIP.js, requires-body = false, binary-body-mode = false, timeout = 20, tag = 会员皮肤, enable={Music163_VIP_Shared} ,argument=[{Cookie},{MConfigInfo},{UserAgent}]

# 搜索结果会员歌曲
http-request ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/search\/(?:complex\/page|complex\/rec\/song\/get|song\/list\/page) script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/CloudMusicVIP.js, requires-body = false, binary-body-mode = false, timeout = 20, tag = 搜索结果, enable={Music163_VIP_Shared} ,argument=[{Cookie},{MConfigInfo},{UserAgent}]

# 侧边栏会员等级
http-request ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/vipnewcenter\/app\/resource\/newaccountpage script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/CloudMusicVIP.js, requires-body = false, binary-body-mode = false, timeout = 20, tag = 侧边会员, enable={Music163_VIP_Shared} ,argument=[{Cookie},{MConfigInfo},{UserAgent}]

# 歌单列表会员认证
http-request ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/w?e?api\/(?:homepage\/|v6\/)?playlist\/(?!(?:delete|subscribe|unsubscribe)) script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/CloudMusicVIP.js, requires-body = false, binary-body-mode = false, timeout = 20, tag = 列表会员, enable={Music163_VIP_Shared} ,argument=[{Cookie},{MConfigInfo},{UserAgent}]

# 会员认证
http-request ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/vipauth\/app\/auth\/(soundquality\/)?query script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/CloudMusicVIP.js, requires-body = false, binary-body-mode = false, timeout = 20, tag = 会员认证, enable={Music163_VIP_Shared} ,argument=[{Cookie},{MConfigInfo},{UserAgent}]

#----------  广告过滤 ----------#
# 热推、有话想说、分享一下、歌曲下的祝福等小提示、评论区、乐迷、星评等级、关注等图标
http-response ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/(?:batch|v2\/resource\/comment\/floor\/get) script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/NeteaseMusicAds.js, requires-body = true, binary-body-mode = true, timeout = 20, tag = 网易评论

# 推荐、home、主页
http-response ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/(?:homepage\/block\/page|link\/page\/rcmd\/(?:block\/resource\/multi\/refresh|resource\/show)) script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/NeteaseMusicAds.js, requires-body = true, binary-body-mode = true, timeout = 20, tag = 网易主页, argument=[{PRGG},{PRRK},{PRDRD},{PRSCVPT},{PRST},{PRRR},{HMPR},{PRMST},{PRCN}]

# 底部Tab
http-response ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/link\/home\/framework\/tab script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/NeteaseMusicAds.js, requires-body = true, binary-body-mode = true, timeout = 20, tag = 底部拉环, argument=[{MY},{DT},{FX},{GZ}]

# 发现页
http-response ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/link\/page\/discovery\/resource\/show script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/NeteaseMusicAds.js, requires-body = true, binary-body-mode = true, timeout = 20, tag = 发现页面

# 播放音效
# http-response ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/song\/play\/more\/list\/v\d script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/NeteaseMusicAds.js, requires-body = true, binary-body-mode = true, timeout = 20, tag = 播放音效

# 我的页面
http-response ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/link\/position\/show\/resource script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/NeteaseMusicAds.js, requires-body = true, binary-body-mode = true, timeout = 20, tag = 我的页面

# 显示未关注你的人
http-response ^https?:\/\/(?:ipv4|interface\d?)\.music\.163\.com\/eapi\/user\/follow\/users\/mixed\/get script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NeteaseMusic/NeteaseMusicAds.js, requires-body = true, binary-body-mode = true, timeout = 20, tag = 显示未关
