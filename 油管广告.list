#!name=油管广告
#!desc=油管去广告
#!author=Maasea[https://github.com/Maasea], VirgilClyne[https://github.com/VirgilClyne], Choler[https://github.com/Choler], DivineEngine[https://github.com/DivineEngine], app2smile[https://github.com/app2smile]
#!icon=https://rx.nzvrn.com/x/youtube
#!openUrl=https://youtube.com
#!homepage=https://hub.kelee.one
[Argument]
blockUpload=switch,"false","true",tag=上传按钮, desc=用于隐藏YouTube底栏的上传按钮
blockShorts=switch,"false","true",tag=短视按钮, desc=用于隐藏YouTube底栏的Shorts按钮
blockImmersive=switch,"false","true",tag=选段按钮, desc=用于隐藏YouTube Music底栏的选段按钮
captionLang=select, "zh-Hans", "zh-Hant", "ja", "ko", "en", "off",tag=字幕翻译语言, desc=需要把YouTube字幕翻译翻译为何种语言
lyricLang=select, "zh-Hans", "zh-Hant", "ja", "ko", "en", "off",tag=歌词翻译语言, desc=需要把YouTube Music字幕翻译翻译为何种语言
debug=switch,"false","true",tag=调试模式, desc=此插件的调试模式
[Rule]
AND,((DOMAIN-SUFFIX,googlevideo.com),(PROTOCOL,QUIC)),REJECT
AND,((DOMAIN-SUFFIX,youtubei.googleapis.com),(PROTOCOL,QUIC)),REJECT
[MITM]
hostname = *.googlevideo.com, youtubei.googleapis.com
[Rewrite]
^https?:\/\/[\w-]+\.googlevideo\.com\/initplayback.+&oad - reject-200
[Script]
http-response ^https:\/\/youtubei\.googleapis\.com\/(youtubei\/v1\/(browse|next|player|search|reel\/reel_watch_sequence|guide|account\/get_setting|get_watch))(\?(.*))?$ script-path=https://kelee.one/Resource/JavaScript/YouTube/YouTube_remove_ads/YouTube_remove_ads_response.js, requires-body=true, binary-body-mode=true, tag=油管去广, argument="[{blockUpload},{blockShorts},{blockImmersive},{captionLang},{lyricLang},{debug}]"
