#!name=油管广告
#!desc=油管去广告
#!author=Maasea[https://github.com/Maasea], VirgilClyne[https://github.com/VirgilClyne], Choler[https://github.com/Choler], DivineEngine[https://github.com/DivineEngine], app2smile[https://github.com/app2smile]
#!icon=https://rx.nzvrn.com/x/youtube
#!homepage=https://hub.kelee.one
[Argument]
blockUpload = switch, true, false, tag = 上传按钮, desc = 用于隐藏YouTube底栏的上传按钮
blockShorts = switch, true, false, tag = 短视按钮, desc = 用于隐藏YouTube底栏的Shorts按钮
blockImmersive = switch, true, false, tag = 选段按钮, desc = 用于隐藏YouTube Music底栏的选段按钮
captionLang = select, "zh-Hans", "zh-Hant", "ja", "ko", "en", "off", tag = 字幕翻译, desc = 选择翻译为何种语言字幕
lyricLang = select, "zh-Hans", "zh-Hant", "ja", "ko", "en", "off", tag = 歌词翻译, desc = 选择翻译为何种语言字幕
debug = switch, false, true, tag = 调试模式, desc = 此插件的调试模式
[MitM]
hostname = *.googlevideo.com, youtubei.googleapis.com
[Rule]
AND, ((DOMAIN-SUFFIX, googlevideo.com),  (PROTOCOL, QUIC)), REJECT
AND, ((DOMAIN-SUFFIX, youtubei.googleapis.com),  (PROTOCOL, QUIC)), REJECT
[Rewrite]
^https?:\/\/[\w-]+\.googlevideo\.com\/initplayback.+&oad reject-200
[Script]
http-response ^https:\/\/youtubei\.googleapis\.com\/(youtubei\/v1\/(browse|next|player|search|reel\/reel_watch_sequence|guide|account\/get_setting|get_watch))(\?(.*))?$ script-path = https://xzx.nzvrn.com/https://raw.githubusercontent.com/Maasea/sgmodule/master/Script/Youtube/youtube.response.js, requires-body = true, binary-body-mode = true, tag = 油管广告, argument=[{blockUpload},{blockShorts},{blockImmersive},{captionLang},{lyricLang},{debug}]
